<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <solid
                android:color="@android:color/darker_gray" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>


    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="#1b6f9d"/>
            <corners
                android:radius="3dp" />
        </shape>
    </item>

</layer-list>